{
	"server_name": [
		"www.hansuru.com hansuru.com"
	],
	"listen": 80,
	"statics": [
		{
			"location": "/",
			"root": "/apps/hansuru/dist"
		}
	],
	"proxys": [
		{
			"location": "/api/",
			"backends_name": "backend",
			"backends": [
				{
					"address": "127.0.0.1",
					"port": 8280
				}
			]
		}
	],
	"globals": {
		"trust_proxy": true
	},
	"logs": {
		"access": {
			"path": "logs/acc.pkjs.log"
		},
		"error": {
			"path": "logs/err.pkjs.log"
		}
	}
}
